<button data-test="testStarred" value={{on}} type="button" onclick={{action "toggle"}}>
  {{#if @on}}
    <img data-test="starFilled" alt="starFilled" src="assets/images/starfilled.png" width="15" height="15">
  {{else}}
    <img data-test="starUnfilled" alt="star" src="assets/images/star.png" width="15" height="15">
  {{/if}}
</button>
